{
  "name": "dicey",
  "description": "Pen-and-paper roleplaying game aid - dice roller.",
  "version": "3.1.3",
  "scripts": {
    "start": "npm prune && npm install && npm run server",
    "server": "./node_modules/.bin/ws --key ws.key --cert ws.crt -z -n -d public/",
    "watch": "node ./scripts/watch.js",
    "minify": "./node_modules/.bin/minify ./public/bundle.js -o ./public/bundle.min.js",
    "ncu": "ncu --packageFile package.json --loglevel verbose",
    "ncuu": "ncu --packageFile package.json --loglevel verbose -u"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-minify": "^0.2.0",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "droll": "^0.2.1",
    "envify": "^4.1.0",
    "firebase": "^4.8.1",
    "local-web-server": "^2.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sw-precache": "^5.2.0",
    "uglify-js": "^3.2.2",
    "uglifyify": "^4.0.5",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "create-react-class": "^15.6.2",
    "prop-types": "^15.6.0"
  }
}
